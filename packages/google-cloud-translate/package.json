{
  "name": "@google-cloud/translate",
  "description": "Cloud Translation API Client Library for Node.js",
  "version": "7.2.0",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "type": "module",
  "engines": {
    "node": ">=14.18.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/esm/src/index.d.ts",
        "default": "./build/esm/src/index.js"
      },
      "require": {
        "types": "./build/cjs/src/index.d.ts",
        "default": "./build/cjs/src/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "directory": "packages/google-cloud-translate",
    "url": "https://github.com/googleapis/google-cloud-node.git"
  },
  "main": "./build/cjs/src/index.cjs",
  "types": "./build/cjs/src/index.d.ts",
  "files": [
    "build/esm",
    "build/cjs",
    "!build/esm/**/*.map"
  ],
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "google translate",
    "translate"
  ],
  "scripts": {
    "docs": "jsdoc -c .jsdoc.js",
    "predocs": "npm run compile",
    "lint": "gts check",
    "samples-test": "npm run compile && cd samples/ && npm link ../ && npm i && npm test",
    "system-test": "c8 mocha build/cjs/system-test",
    "test:cjs": "c8 mocha build/cjs/test",
    "test:esm": "c8 mocha --loader=esmock build/esm/test",
    "test": "npm run test:cjs && npm run test:esm",
    "compile:esm": "tsc -p ./tsconfig.esm.json",
    "compile:cjs": "tsc -p ./tsconfig.json",
    "compile": "npm run compile:esm && npm run compile:cjs && cp -r protos build/esm && cp -r protos build/cjs && minifyProtoJson build/cjs && minifyProtoJson build/esm && node post-compile.js",
    "compile-protos": "compileProtos src",
    "fix": "gts fix",
    "prepare": "npm run compile-protos && npm run compile",
    "pretest": " npm run compile",
    "presystem-test": "npm run compile",
    "docs-test": "linkinator docs",
    "predocs-test": "npm run docs",
    "prelint": "cd samples; npm link ../; npm install",
    "clean": "gts clean",
    "precompile": "gts clean"
  },
  "dependencies": {
    "@google-cloud/common": "^4.0.0",
    "@google-cloud/promisify": "^3.0.0",
    "extend": "^3.0.2",
    "google-gax": "^3.5.8",
    "html-tags": "^3.3.1"
  },
  "devDependencies": {
    "@types/extend": "^3.0.0",
    "@types/is-html": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/request": "^2.47.1",
    "@types/sinon": "^10.0.0",
    "c8": "^7.0.0",
    "codecov": "^3.0.2",
    "esmock": "^2.2.1",
    "google-auth-library": "^8.0.0",
    "gts": "^3.1.0",
    "http2spy": "^2.0.0",
    "jsdoc": "^4.0.0",
    "jsdoc-fresh": "^2.0.0",
    "jsdoc-region-tag": "^2.0.0",
    "linkinator": "^4.0.0",
    "mocha": "^9.2.2",
    "pack-n-play": "^1.0.0-2",
    "sinon": "^15.0.0",
    "typescript": "^4.9.5"
  },
  "homepage": "https://github.com/googleapis/google-cloud-node/tree/main/packages/google-cloud-translate"
}
